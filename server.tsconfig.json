{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2019"
        ],
        "rootDir": "./function/server",
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "function/server/build",
        "app/",
        "tailwind.config.ts"
    ]
}